# SubPaperFlux Prod profile (.env)
# Copy to env/prod.env (or similar) and reference via docker compose env_file.

SPF_PROFILE=prod
NEXT_PUBLIC_SPF_PROFILE=${SPF_PROFILE}

# API base exposed to the UI
API_BASE=https://api.example.com
NEXT_PUBLIC_API_BASE=https://api.example.com

# Production OIDC configuration (replace with your provider)
OIDC_ISSUER=https://auth.example.com
OIDC_CLIENT_ID=subpaperflux
OIDC_CLIENT_SECRET=prod-secret
OIDC_AUDIENCE=subpaperflux

# Feature toggles
USER_MGMT_CORE=1
USER_MGMT_UI=1
NEXT_PUBLIC_USER_MGMT_UI=1
USER_MGMT_OIDC_ONLY=1
SCIM_ENABLED=1
SCIM_WRITE_ENABLED=1

